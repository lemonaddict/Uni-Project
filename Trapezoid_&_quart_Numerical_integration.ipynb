      "source": [
        "#@title Simpson's 1/3 rule\n",
        "\n",
        "a = 0\n",
        "b = np.pi\n",
        "n = 11\n",
        "h = (b-a) / (n-1)\n",
        "x = np.linspace(a,b,n)\n",
        "f = np.sin(x)\n",
        "\n",
        "I_simp = (h/3)*(f[0]+ 2* sum(f[:n-2:2]) +4*sum(f[1:n-1:2])+ f[n-1])\n",
        "err_simp = 2 - I_simp\n",
        "print(I_simp)\n",
        "print(err_simp)"
      ],
